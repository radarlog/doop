{
    "name": "doop",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">= 14.5",
        "yarn": ">= 1.22"
    },
    "scripts": {
        "dev-server": "nodemon --exec 'encore dev-server --keep-public-path --config config/encore.js'",
        "dev": "encore dev --config config/encore.js",
        "watch": "encore dev --watch --config config/encore.js",
        "build": "encore production --progress --config config/encore.js",
        "lint": "eslint src/Frontend/ts/"
    },
    "dependencies": {
        "bootstrap": "^4.5.3",
        "bs-custom-file-input": "^1.3.4",
        "jquery": "^3.5.1"
    },
    "devDependencies": {
        "@symfony/webpack-encore": "^0.31.0",
        "@types/jquery": "^3.5.3",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "core-js": "^3.6.5",
        "cssnano": "^4.1.10",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.6",
        "popper.js": "^1.16.1",
        "postcss-loader": "^4.0.4",
        "prettier": "^2.1.2",
        "sass": "^1.27.0",
        "sass-loader": "^10.0.4",
        "ts-loader": "^8.0.6",
        "typescript": "^4.0.3"
    },
    "nodemonConfig": {
        "watch": [
            "config/encore.js",
            "tsconfig.json"
        ]
    }
}
