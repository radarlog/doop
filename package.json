{
    "name": "doop",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">= 15.5",
        "yarn": ">= 1.22"
    },
    "scripts": {
        "dev-server": "nodemon --exec 'encore dev-server --keep-public-path --config config/encore.js'",
        "dev": "encore dev --config config/encore.js",
        "watch": "encore dev --watch --config config/encore.js",
        "build": "encore production --progress --config config/encore.js",
        "lint": "eslint src/Frontend/ts/"
    },
    "dependencies": {
        "bootstrap": "^4.6",
        "bs-custom-file-input": "^1.3",
        "jquery": "^3.6"
    },
    "devDependencies": {
        "@symfony/webpack-encore": "^1.4",
        "@types/jquery": "^3.5",
        "@typescript-eslint/eslint-plugin": "^4.26",
        "@typescript-eslint/parser": "^4.26",
        "core-js": "^3.13",
        "cssnano": "^5.0",
        "eslint": "^7.27",
        "eslint-config-prettier": "^8.3",
        "eslint-plugin-prettier": "^3.4",
        "nodemon": "^2.0",
        "popper.js": "^1.16",
        "postcss": "^8.3",
        "postcss-loader": "^5.3",
        "prettier": "^2.3",
        "sass": "^1.34",
        "sass-loader": "^12.0",
        "ts-loader": "^9.2",
        "typescript": "^4.3"
    },
    "nodemonConfig": {
        "watch": [
            "config/encore.js",
            "tsconfig.json"
        ]
    }
}
