parameters:
    tmpDir: var/cache/phpstan

    paths:
        - bin
        - public
        - src
        - tests

    level: 5

    ignoreErrors:
        - message: '/PHPUnit\\Framework\\MockObject\\MockObject given/'
          path: tests/Application/Command/LoggerBusTest.php
        - message: '/Result of || is always true/'
          path: src/Infrastructure/Kernel.php
