env:
    browser: true
    es6: true
extends:
    - plugin:@typescript-eslint/recommended
    - prettier/@typescript-eslint
    - plugin:prettier/recommended
parser: "@typescript-eslint/parser"
parserOptions:
    ecmaVersion: 2020
    project: tsconfig.json
plugins:
    - "@typescript-eslint"
    - prettier
rules:
    prettier/prettier: [error, singleQuote: true]
