{
    "name": "radarlog/doop",
    "description": "Simple File Uploader / AWS S3 Uploader",
    "keywords": [
        "AWS",
        "S3 Uploader",
        "Symfony",
        "Hexagonal Architecture",
        "CQRS"
    ],
    "type": "project",
    "license": "MIT",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "~7.3",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.98",
        "doctrine/dbal": "^2.9",
        "doctrine/migrations": "^2.0",
        "psr/log": "^1.1",
        "ramsey/uuid": "^3.8",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/flex": "^1.2",
        "symfony/form": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/monolog-bundle": "^3.3",
        "symfony/translation-contracts": "^1.1",
        "symfony/twig-bundle": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "slevomat/coding-standard": "^4.8",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/browser-kit": "^4.3",
        "symfony/dotenv": "^4.3",
        "thomas-schulz/symfony-meta": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Radarlog\\Doop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radarlog\\Doop\\Tests\\": "tests"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
